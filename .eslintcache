ter + 1) : (counter -1);\r\n\r\n    return(\r\n        <button type=\"button\" class=\"btn btn-primary btn-sm\"onClick={onClick(amount)}>{sign}</button>\r\n        )\r\n}","C:\\Users\\Juampi\\Desktop\\Cursos\\Curso React\\Clase 3\\Desafio clase 3\\my-app\\src\\components\\ItemList.js",[],"C:\\Users\\Juampi\\Desktop\\Cursos\\Curso React\\Clase 3\\Desafio clase 3\\my-app\\src\\components\\Item.js",[],"C:\\Users\\Juampi\\Desktop\\Cursos\\Curso React\\Clase 3\\Desafio clase 3\\my-app\\src\\components\\ItemDetailContainer.js",["75"],"import React, { useState, useEffect } from \"react\";\r\nimport ItemDetail from \"./ItemDetail\";\r\n\r\n//Este componente contiene la vista de un sólo producto al que llegamos después de hacer click sobre el mismo en ItemList. Toma el id del link y hace el fecth para traer la info de ese solo producto. Usa esa info para mapear en compoennte hijo => ItemDetail.\r\nfunction ItemDetailContainer(props) {\r\n  const [item, setItem] = useState(false);\r\n  let id = props.match.params.id;\r\n\r\n  useEffect(() => {\r\n    setTimeout(() => {\r\n      fetch(`https://5f3c95f36c11f80016d6f21e.mockapi.io/bitbuyer/items/${id}`)\r\n        .then(response => {\r\n          return response.json();\r\n        })\r\n        .then(data => {\r\n          setItem(data);\r\n        });\r\n    }, 3000);\r\n    console.log(item);\r\n  }, []); //Esto se ejecuta apenas se monta el componente / onInit\r\n\r\n  return (\r\n    <div>\r\n      {item ? (\r\n        <ItemDetail nombre={item.nombre} precio={item.precio} />\r\n      ) : (\r\n        <p>Trayendo información desde base de datos...</p>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default ItemDetailContainer;\r\n","C:\\Users\\Juampi\\Desktop\\Cursos\\Curso React\\Clase 3\\Desafio clase 3\\my-app\\src\\components\\ItemDetail.js",[],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":1,"column":17,"nodeType":"82","messageId":"83","endLine":